//possibile efficientarlo ulteriormente con ciò che abbiamo già visto

function reverseArray(a) {
    let n=0;   
       for (const element of a){
           n++
       }
       for (let i=0; i<n-1; i++){
           a.unshift(a[(2*i)+1]);
       }
       for(let i=0; i<n-1; i++)
       {
           a.pop()
       }
       return a;
   }